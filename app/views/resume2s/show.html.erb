          <% end %>
        </div>
      </div>
      <div class="right">
        <div class="name-div">
          <h1><%= @resume2.name %></h1>
          <p><%= @resume2.current_role %></p>
        </div>
        <div class="box-2">
          <h2>ABOUT ME <i class="fa fa-user icons3" style="font-size: 28px!important;">Call</i></h2>
